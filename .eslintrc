{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off"
  },
  "env": {
    "jest": true
  },
  "overrides": [
    {
      "files": ["babel.config.js"],
      "env": {
        "commonjs": true
      }
    }
  ]
}
